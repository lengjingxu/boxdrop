<div id="answer-22421121" class="answer" data-answerid="22421121"  itemscope itemtype="http://schema.org/Answer">

    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="22421121">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
